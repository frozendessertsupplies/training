<div class="title"><h3>{{title}}</h3></div>
<div class="question"><h4>{{question}}</h4></div>
{{#if mChoice}}
<div class="options">
    {{#each answers}}
        <input type="radio" name="question" value="{{this}}"> {{this}}
    {{/each}}
</div>
{{/if}}
{{#if fillBlank}}
    <input type="text">
{{/if}}
